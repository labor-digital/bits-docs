{
	"compilerOptions": {
		"outDir": "dist",
		"strict": true,
		"sourceMap": true,
		"target": "es5",
		"experimentalDecorators": true,
		"suppressImplicitAnyIndexErrors": true,
		"moduleResolution": "node",
		"module": "esnext",
		"jsx": "react",
		"skipDefaultLibCheck": true,
		"useUnknownInCatchVariables": false,
		"skipLibCheck": true,
		"allowJs": true
	},
	"include": [
		"./src/**/*"
	]
}